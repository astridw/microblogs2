<h1>Sessions#signup</h1>

<%= form_for @user, url: create_user_path do |f| %>

<p>
  <%= f.label :username %>
  <%= f.text_field :username %>
</p>
<p>
  <%= f.label :password %>
  <%= f.text_field :password %>
</p>
<p>
  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>
</p>
<p>
  <%= f.submit %>
</p>

<% end %>
